{"workflow":{"nodes":[{"name":"pair1_fastp","cmd":"fastp --in1 #PAIR1_R1_FILE# --in2 #PAIR1_R2_FILE# --out1 [#SAMPLE_NAME#.pair1.r1.clean] --out2 [#SAMPLE_NAME#.pair1.r2.clean]"},{"name":"pair2_fastp","cmd":"fastp --in1 #PAIR2_R1_FILE# --in2 #PAIR2_R2_FILE# --out1 [#SAMPLE_NAME#.pair2.r1.clean] --out2 [#SAMPLE_NAME#.pair2.r2.clean]"},{"name":"pari1_bwa_mem","cmd":"bwa mem #BWA_MEM_PARAMS# -R '@RG\\tID:#SAMPLE_NAME#.pair1\\tLB:#SAMPLE_NAME#.pair1\\tSM:#SAMPLE_NAME#.pair1\\tPL:#PLATFORM#' #REF_GENOME# <#SAMPLE_NAME#.pair1.r1.file> <#SAMPLE_NAME#.pair1.r2.file> | samtools view -Shb -o [#SAMPLE_NAME#.pair1.bwamem.bam] -"},{"name":"pair2_bwa_mem","cmd":"bwa mem #BWA_MEM_PARAMS# -R '@RG\\tID:#SAMPLE_NAME#.pair1\\tLB:#SAMPLE_NAME#.pair1\\tSM:#SAMPLE_NAME#.pair1\\tPL:#PLATFORM#'#REF_GENOME# <#SAMPLE_NAME#.pair2.r1.file> <#SAMPLE_NAME#.pair2.r2.file> | samtools view -Shb -o [#SAMPLE_NAME#.pair2.bwamem.bam] -"},{"name":"pair1_applybqsr","cmd":"gatk --java-options \"#GATK_JAVA_OPTS#\" ApplyBQSR -R #REF_GENOME# #GATK_APPLYBQSR_PARAMS# -I <in1> --bqsr-recal-file <in2> -O [#SAMPLE_NAME#.pair1.bam]"},{"name":"pair2_applybqsr","cmd":"gatk --java-options \"#GATK_JAVA_OPTS#\" ApplyBQSR -R #REF_GENOME# #GATK_APPLYBQSR_PARAMS# -I <in1> --bqsr-recal-file <in2> -O [#SAMPLE_NAME#.pair2.bam]"},{"name":"pair1_baserecalibrator","cmd":"gatk --java-options \"#GATK_JAVA_OPTS#\" BaseRecalibrator -R #REF_GENOME# #GATK_RECALIBRATOR_PARAMS# -I <in> -O [#SAMPLE_NAME#.pair1.gatkbr.table] -L #BED_FILE#"},{"name":"pair2_baserecalibrator","cmd":"gatk --java-options \"#GATK_JAVA_OPTS#\" BaseRecalibrator -R #REF_GENOME# #GATK_RECALIBRATOR_PARAMS# -I <in> -O [#SAMPLE_NAME#.pair2.gatkbr.table] -L #BED_FILE#"},{"name":"varcall","cmd":"VarDict -G #REF_GENOME# #VARDICT_PARAMS# -N #SAMPLE_NAME#.pair1 -b \"<#SAMPLE_NAME#.pair1.bam>|<#SAMPLE_NAME#.pair2.bam>\" #BED_FILE# | testsomatic.R | var2vcf_paired.pl #VAR2VCF_PARAMS# -N \"#SAMPLE_NAME#.pair1|#SAMPLE_NAME#.pair2\" > [#SAMPLE_NAME#.vardict.vcf]"},{"name":"pai